# Physical Esc mapping (instead of Â± key on Macbook Pro with TouchBar)
0x0A: skhd -k 0x35
0x3F: skhd -k 0x3B
0x3B: skhd -k 0x3F

# Move between yabai spaces
alt - 1: yabai -m space --focus 1
alt - 2: yabai -m space --focus 2
alt - 3: yabai -m space --focus 3
alt - 4: yabai -m space --focus 4
alt - 5: yabai -m space --focus 5


